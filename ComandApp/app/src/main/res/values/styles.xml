<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="android:textAllCaps">false</item>
        <!--<item name="android:actionBarStyle">@style/MyActionBarLogo</item>-->
    </style>

    <style name="MyActionBarLogo" parent="@style/Widget.AppCompat.Light.ActionBar">
        <item name="displayOptions">useLogo|showHome</item>
    </style>

    <style name="AppTheme2" parent="@android:style/Theme.Holo">
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="botonCustom">
        <item name="android:background">@drawable/estilo_botones</item>
        <item name="android:textColor">#634400</item>
    </style>

    <style name="botonCustomSubMenu">
        <item name="android:background">@drawable/estilo_botones_submenu</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">14dp</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:drawablePadding">6dp</item>
        <item name="android:paddingLeft">6dp</item>
    </style>

    <style name="nombreBaresLista">
        <item name="android:textColor">#737373</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>
